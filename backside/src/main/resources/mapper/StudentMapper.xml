<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.study.backside.mapper.StudentMapper">


    <!--添加学生记录-->
    <insert id="addStudent" parameterType="com.study.backside.bean.Student">
        insert into student (number,name,college,major,e_mail,tel)
        values (#{num}, #{nm}, #{clg}, #{mj}, #{em}, #{te})
    </insert>

</mapper>